      "mentions": []
    },
    {
      "id": "874284037018103848",
      "type": "Default",
      "timestamp": "2021-08-09T13:32:37.837+00:00",
      "timestampEdited": null,
      "callEndedTimestamp": null,
      "isPinned": false,
      "content": "`// 以下、ローカル関数の宣言のみ` みたいなコメントを入れたらいいのかな。",
      "author": {
        "id": "293624673265123328",
        "name": "koher",
        "discriminator": "9336",
        "nickname": "koher",
        "color": "#11806A",
        "isBot": false,
        "avatarUrl": "https://cdn.discordapp.com/avatars/293624673265123328/accd07acc220a18568ba46a6e9ede18a.png?size=128"
      },
      "attachments": [],
